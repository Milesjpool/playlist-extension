{
  "manifest_version": 2,

  "name": "Friday beers playlist",
  "description": "",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["https://www.7digital.com/*"],
      "js": ["jquery-1.11.3.min.js", "playlister.js"]
    }
  ],
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}